<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title><%=name%></title>
  </head>
  <body>
    <% if(profileUser) { %>
    <h1>Name: <%=name%></h1>
    <ul>
      <% flatList.forEach((flat) => { %>
      <li><a href="../../flat/profile/<%=flat.flatID%>"><%=flat.name%></a></li>
      <% }) %>
    </ul>
    <% } else { %>
    <p>USER NOT FOUND!</p>
    <% } %>
  </body>
</html>
